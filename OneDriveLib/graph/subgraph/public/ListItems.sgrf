variant file;
variant folder;
string type;

// Transforms input record into output record.
function integer transform() {
	$out.0.* = $in.0.*;
	
	// type
	if($in.0.file != null){
		type = "file";
	} else if($in.0.folder != null){
		type = "folder";
	} else {
		type = "unknown";
	}
	
	$out.0.type = type;
	
	return ALL;
