// Transforms input record into output record.
function integer transform() {
	$out.0.* = $in.0.*;
	$out.0.URL = $in.0.webUrl;
	$out.0.lastModified = $in.0.lastModifiedDateTime;
	$out.0.size = str2long($in.0.size);
	
	// type
	$out.0.isDirectory = $in.0.type == "folder" ? true : false;
	$out.0.isFile = $in.0.type == "file" ? true : false;
	
	$out.0.MSDonwloadUrl = $in.0.microsoft_graph_downloadUrl;
	

	return ALL;
}

